<launch>

  <node pkg="laser_scan_matcher" type="laser_scan_matcher_node"
    name="laser_scan_matcher_node" output="screen" respawn="true">

    <param name="fixed_frame" value="odom"/>
    <param name="base_frame" value="base_link"/>

    <param name="use_imu" value="false"/>
    <param name="use_odom" value="false"/>
    <param name="use_vel" value="false"/>

    
        
    <param name="kf_dist_linear" value="1000.0"/>
    <param name="kf_dist_angular" value="1000.0"/>

    <param name="max_iterations" value="5"/>
    <param name="max_correspondence_dist" value="100.0"/>
    <param name="max_angular_correction_deg" value="120"/>
    <param name="max_linear_correction" value="2.0"/>
    <!--<param name="epsilon_xy" value="0.000001"/>
    <param name="epsilon_theta" value="0.0000001"/>-->
        <param name="epsilon_xy" value="0.001"/>
    <param name="epsilon_theta" value="0.001"/>
    <param name="outliers_maxPercs" value="0.9"/>

 <param name="publish_tf" value="true"/>
    


  </node>

</launch>